<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>JustinWritesCode.EntityFrameworkCore.Extensions</AssemblyName>
    <PackageId>$(AssemblyName)</PackageId>
    <EnableGlobalCodeGenerators>false</EnableGlobalCodeGenerators>
  </PropertyGroup>
  <ItemGroup>
		<PackageReference Include="Microsoft.EntityFrameworkCore" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Relational" />
    <PackageReference Include="JustinWritesCode.Extensions"  ExcludeAssets="ContentFiles" />
    <PackageReference Include="JustinWritesCode.Abstractions"  ExcludeAssets="ContentFiles" />
  </ItemGroup>
</Project>
